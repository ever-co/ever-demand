		"start": "yarn run build && ng serve --port 4201",
		"build": "ng build",
		"build:prod": "ng build --prod --stats-json",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"bundle-report": "webpack-bundle-analyzer www/stats.json",

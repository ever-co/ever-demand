{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": ["es2017"],
		"allowJs": true,
		"jsx": "react-native",
		"noEmit": true,
		"isolatedModules": true,
		"strict": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules", "babel.config.ts", "metro.config.ts"],
	"extends": "expo/tsconfig.base"
}
